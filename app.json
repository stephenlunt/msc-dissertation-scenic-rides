{
  "expo": {
    "name": "Scenic Rides",
    "slug": "scenic-rides",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.stephenlunt.scenicrides"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.stephenlunt.scenicrides"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Scenic Rides to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "accb6944-f5ff-457b-ae01-6a5c00ecbb72"
      }
    }
  }
}
